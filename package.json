{
  "name": "swift-suds",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "_build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write",
    "prettier": "prettier \"**/*.tsx\"",
    "eslint": "eslint . --ext .tsx",
    "build:css": "postcss tailwind.css -o src/theme/tailwind.css",
    "watch:css": "postcss tailwind.css -o src/theme/tailwind.css -w",
    "start:dev": "npm run watch:css && ionic serve"
  },
  "dependencies": {
    "@azure/msal-browser": "^3.4.0",
    "@azure/msal-react": "^2.0.7",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.5.1",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.6",
    "@capacitor/status-bar": "5.0.6",
    "@ionic/core": "^8.3.0",
    "@ionic/react": "^8.3.0",
    "@ionic/react-router": "^8.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "dayjs": "^1.11.11",
    "ionicons": "^7.0.0",
    "msal": "^1.4.18",
    "postcss-nesting": "^12.1.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "styled-components": "^6.1.0",
    "twin.macro": "^3.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@capacitor/cli": "5.5.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.16",
    "cypress": "^12.7.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-react-app": "^7.0.1",
    "jsdom": "^22.1.0",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.32.2"
  },
  "description": "SwiftSuds - On Demand Laundry Pickup and Delivery",
  "babelMacros": {
    "twin": {
      "config": "./src/tailwind.config.cjs",
      "preset": "styled-components"
    }
  }
}
